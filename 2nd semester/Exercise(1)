#include <iostream>

using namespace std;

int main() {
    int m,n,x;
    
    cin >> m >> n >> x;
    
    int a[m][n];
    
    for(int i = 0; i < m; i++){
        for(int j = 0; j < n; j ++){
            cin >> a[i][j];
            a[i][j] = a[i][j]*x;
        }
    }
    
    for(int i = 0; i < m; i++){
        for(int j = 0; j < n; j ++){
            cout << a[i][j] << " ";
        }
        cout << endl;
    }
}
